{
  "name": "infomaniak-ai-provider",
  "version": "0.1.1",
  "packageManager": "pnpm@10.14.0",
  "license": "Apache-2.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/wireless25/infomaniak-ai-provider.git"
  },
  "keywords": [
    "ai",
    "infomaniak",
    "ai-sdk-provider",
    "ai-sdk",
    "infomaniak-ai-tools"
  ],
  "sideEffects": false,
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    }
  },
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "files": [
    "CHANGELOG.md",
    "dist/**/*"
  ],
  "engines": {
    "node": ">=18"
  },
  "scripts": {
    "build": "pnpm clean && generate-types && tsup",
    "build:watch": "pnpm clean && generate-types && tsup --watch",
    "clean": "rm -rf dist *.tsbuildinfo",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "generate-types": "pnpx tsx scripts/generate-types.ts",
    "typecheck": "tsc --noEmit",
    "test:update": "pnpm test:node -u",
    "test:watch": "vitest",
    "test": "vitest --run",
    "release": "pnpx changelogen@latest --release && pnpm publish",
    "postinstall": "pnpm generate-types"
  },
  "peerDependencies": {
    "zod": "^3.25.76 || ^4"
  },
  "dependencies": {
    "@ai-sdk/openai-compatible": "^1.0.6",
    "@ai-sdk/provider": "latest",
    "@ai-sdk/provider-utils": "latest"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^5.2.1",
    "@types/node": "20.17.24",
    "dotenv": "^17.2.1",
    "eslint": "^9.33.0",
    "tsup": "^8",
    "typescript": "5.8.3",
    "vite": "^7.1.2",
    "vitest": "^3.2.4",
    "zod": "3.25.76"
  },
  "publishConfig": {
    "access": "public"
  }
}
